{
    "name": "kreait/gcp-metadata",
    "description": "Get the metadata from a Google Cloud Platform environment.",
    "keywords": ["google", "google-cloud", "google-cloud-engine", "gce", "google-cloud-platform", "gcp"],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Jérôme Gamez",
            "email": "jerome@gamez.name"
        }
    ],
    "require": {
        "php": "^7.4|^8.0",
        "guzzlehttp/guzzle": "^6.5.8|^7.4.5",
        "psr/http-message": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5.20"
    },
    "autoload": {
        "psr-4": {
            "Kreait\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Kreait\\": "tests"
        }
    },
    "scripts": {
        "tests": "vendor/bin/phpunit"
    }
}
